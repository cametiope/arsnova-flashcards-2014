<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description></description>
  <keepDependencies>false</keepDependencies>
  <properties/>
  <scm class="hudson.plugins.cloneworkspace.CloneWorkspaceSCM" plugin="clone-workspace-scm@0.6">
    <parentJobName>THMcards</parentJobName>
    <criteria>Any</criteria>
  </scm>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers>
    <jenkins.triggers.ReverseBuildTrigger>
      <spec></spec>
      <upstreamProjects>THMcards.jmeter, </upstreamProjects>
      <threshold>
        <name>SUCCESS</name>
        <ordinal>0</ordinal>
        <color>BLUE</color>
        <completeBuild>true</completeBuild>
      </threshold>
    </jenkins.triggers.ReverseBuildTrigger>
  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <EnvInjectBuilder plugin="envinject@1.89">
      <info>
        <propertiesContent>CCTRL_EMAIL=&quot;your-mail@example.com&quot;
CCTRL_PASSWORD=&quot;your-awesome-pw&quot;
CCTRL_APP=&quot;your-awesome-appname&quot;
CCTRL_DEPLOYMENT=&quot;your-awesome-deploymentname&quot;</propertiesContent>
      </info>
    </EnvInjectBuilder>
    <hudson.tasks.Shell>
      <command>eval &quot;rm ~/.ssh/cckey&quot;;
eval &quot;rm ~/.ssh/cckey.pub&quot;;
eval &quot;ssh-keygen -t rsa -C &quot;your_email@example.com&quot; -N &quot;asdasd&quot; -f ~/.ssh/cckey&quot;
eval &quot;CCTRL_EMAIL=$CCTRL_EMAIL CCTRL_PASSWORD=$CCTRL_PASSWORD cctrluser key.add ~/.ssh/cckey.pub&quot;
eval &quot;cd /vagrant&quot;
eval &quot;CCTRL_EMAIL=$CCTRL_EMAIL CCTRL_PASSWORD=$CCTRL_PASSWORD cctrlapp $CCTRL_APP/$CCTRL_DEPLOYMENT push&quot;;
eval &quot;CCTRL_EMAIL=$CCTRL_EMAIL CCTRL_PASSWORD=$CCTRL_PASSWORD cctrlapp $CCTRL_APP/$CCTRL_DEPLOYMENT deploy&quot;;</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers/>
  <buildWrappers/>
</project>