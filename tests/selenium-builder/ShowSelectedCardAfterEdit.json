{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://localhost:3000/"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "#cardSetView tr td:first-child a"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "rightCarouselControl"
      }
    },
    {
      "type": "waitForTextPresent",
      "text": "2/"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "div.item.active > div.box > #editCard > i.glyphicon.glyphicon-pencil"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "cardCancel"
      }
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}